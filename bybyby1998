scripts = {
    "Anti - fly",
    "Anti - speed"
}

for _, scriptName in ipairs(scripts) do
    if game:GetService("StarterPlayer").StarterCharacterScripts[scriptName] then
        game:GetService("StarterPlayer").StarterCharacterScripts[scriptName]:remove()
    end
    if game:GetService("Players").LocalPlayer.Character[scriptName] then
        game:GetService("Players").LocalPlayer.Character[scriptName]:remove()
    end
end

print('bypass anticheat success!')
