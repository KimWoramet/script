                        [1] = "Change-World",
                        [2] = {
                            ["World"] = selectedWorldRangerStage
                        }
                    }
                    
                    game:GetService("ReplicatedStorage").Remote.Server.PlayRoom.Event:FireServer(unpack(changeWorld))
