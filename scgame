AutoUpgradeUnitAll:OnChanged(function(UpgradeUnits)
        UpgradeUnit = UpgradeUnits
            task.spawn(function()
                while AutoUpgradeUnitAll.Value do
                    for _, unit in ipairs(unitFolder:GetChildren()) do
                        GetUnitUpgrade:FireServer(unit)
                        task.wait(0.2)
                    end
                 task.wait(5)
                end
            end)
        end)



